import java.util.ArrayList; //import the array list class
import java.util.Arrays;
import java.util.Random; //import the random class
public class randomprisoners {

	public static void main(String[] args) {
		
		for ( int a = 0; a < 10; a++) {
		
			/*names*/
			String[] names = {"Ralph", "Jeremy", "Adam", "Lawrence", "Bobby", "Joshua", "Scott", "Juan", "Peter", "Terry"};	
		
			Random randomNames = new Random();
		
			for (int i = 0; i < names.length; i++) {
				int randomIndexToSwap = randomNames.nextInt(names.length);
				String temp = names[randomIndexToSwap];
				names[randomIndexToSwap] = names[i];
				names[i] = temp;
			}
		
		
			
			/*age*/
			int[] age = {23, 26, 29, 32, 34, 37, 41, 46, 51, 63};	
		
			Random randomAges = new Random();
		
			for (int i = 0; i < age.length; i++) {
				int randomIndexToSwap = randomAges.nextInt(age.length);
				int temp = age[randomIndexToSwap];
				age[randomIndexToSwap] = age[i];
				age[i] = temp;
			}
		
		
			/*personality*/
			String[] personality = {"The Entitled Personality", "The Bully Personality", "The Self-Righteous Personality", "The Sheep Personality"};	
		
			Random randomPersonalities = new Random();
		
			for (int i = 0; i < personality.length; i++) {
				int randomIndexToSwap = randomPersonalities.nextInt(personality.length);
				String temp = personality[randomIndexToSwap];
				personality[randomIndexToSwap] = personality[i];
				personality[i] = temp;
			}
		
		
			/*mood*/
			String[] moods = {"Lonely", "Mad", "High", "Infuriated", "Happy", "Sad"};	
			
			Random randomMood = new Random();
		
			for (int i = 0; i < moods.length; i++) {
				int randomIndexToSwap = randomMood.nextInt(moods.length);
				String temp = moods[randomIndexToSwap];
				moods[randomIndexToSwap] = moods[i];
				moods[i] = temp;
			}
		
		
			/*crime*/
			String[] crimes = {"Larceny", "Drug Possession", "Vandalism", "Fraud", "Property Crimes", "Assault", "Violent Crimes", "Weapon Charges" };	
			
			Random randomCrimes = new Random();
		
			for (int i = 0; i < crimes.length; i++) {
				int randomIndexToSwap = randomCrimes.nextInt(crimes.length);
				String temp = crimes[randomIndexToSwap];
				crimes[randomIndexToSwap] = crimes[i];
				crimes[i] = temp;
			}
			
			/*Sentence Time*/
			int[] sentenceTime = {5, 10, 15, 20, 25, 30, 35, 40, 45, 50};	
			
			Random randomSentenceTime = new Random();
		
			for (int i = 0; i < sentenceTime.length; i++) {
				int randomIndexToSwap = randomSentenceTime.nextInt(sentenceTime.length);
				int temp = sentenceTime[randomIndexToSwap];
				sentenceTime[randomIndexToSwap] = sentenceTime[i];
				sentenceTime[i] = temp;
			}
		
		
			/*foodpreference*/
			String[] foodPreference = {"meat", "fish", "salad", "potatoes", "pasta", "rice", "fruit", "undifferentiated" };	
		
			Random randomFoodPreference = new Random();
		
			for (int i = 0; i < foodPreference.length; i++) {
				int randomIndexToSwap = randomFoodPreference.nextInt(foodPreference.length);
				String temp = foodPreference[randomIndexToSwap];
				foodPreference[randomIndexToSwap] = foodPreference[i];
				foodPreference[i] = temp;
			}
		
		
			/*jobpreference*/
			String[] jobPreference = {"Transcribe Textbooks", "Plant trees", "Cut Hair", "Clean Prison", "Workshop Work", "Cook", "Nursery Job" };	
		
			Random randomJobPreference = new Random();
		
			for (int i = 0; i < jobPreference.length; i++) {
				int randomIndexToSwap = randomJobPreference.nextInt(jobPreference.length);
				String temp = jobPreference[randomIndexToSwap];
				jobPreference[randomIndexToSwap] = jobPreference[i];
				jobPreference[i] = temp;
			}
			
			
			/*SPORTS PREFERENCE*/
			String[] sportsPreference = {"Basketball", "Football", "Gym", "Cardio", "None"};	
			
			Random randomSportsPreference = new Random();
		
			for (int i = 0; i < sportsPreference.length; i++) {
				int randomIndexToSwap = randomSportsPreference.nextInt(sportsPreference.length);
				String temp = sportsPreference[randomIndexToSwap];
				sportsPreference[randomIndexToSwap] = sportsPreference[i];
				sportsPreference[i] = temp;
			}
			
			/*alergies*/
			String[] allergies = {"Gluten", "Animals", "Pollen", "Latex", "Insect stings", "Medications"};	
			
			Random randomAllergies = new Random();
		
			for (int i = 0; i < allergies.length; i++) {
				int randomIndexToSwap = randomAllergies.nextInt(allergies.length);
				String temp = allergies[randomIndexToSwap];
				allergies[randomIndexToSwap] = allergies[i];
				allergies[i] = temp;
			}
			
		
			
			/*contraband*/
			String[] contraband = {"weapons", " drugs", "explosives", "tobacco", "alcohol" , "tattooing equipment", "mobile phones", "nothing"};	
			
			Random randomContraband = new Random();
		
			for (int i = 0; i < contraband.length; i++) {
				int randomIndexToSwap = randomContraband.nextInt(contraband.length);
				String temp = contraband[randomIndexToSwap];
				contraband[randomIndexToSwap] = contraband[i];
				contraband[i] = temp;
			}
			
			
			/*money balance*/
			String[] moneyBalance = {"1k", "2k", "3k", "5k", "10k", "20k", "50k", "none"};	
			
			Random randomMoneyBalance = new Random();
		
			for (int i = 0; i < moneyBalance.length; i++) {
				int randomIndexToSwap = randomMoneyBalance.nextInt(moneyBalance.length);
				String temp = moneyBalance[randomIndexToSwap];
				moneyBalance[randomIndexToSwap] = moneyBalance[i];
				moneyBalance[i] = temp;
			}
			
			System.out.println("PRISONER " + (a+1) + " :" ) ;
			System.out.println(" - Name:  " + names[0]);
			System.out.println(" - Age:  " + age[0]);
			System.out.println(" - Personality:  " + personality[0]);
			System.out.println(" - Mood:  " + moods[0] );
			System.out.println(" - Crime:  " + crimes[0]);
			System.out.println(" - Sentence Time: " + sentenceTime[0]);
			System.out.println(" - Food Preference:  " + foodPreference[0]);
			System.out.println(" - Job Preference:  " + jobPreference[0]);
			System.out.println(" - Sports Preference: " + sportsPreference[0]);
			System.out.println(" - Allergies: " + allergies[0]);
			System.out.println(" - Contraband: " + contraband[0]);
			System.out.println(" - Money Balance: " + moneyBalance[0]);
			System.out.println(" ");
			System.out.println("----------------------------------------");
			System.out.println(" ");

			
		}
	}


}
